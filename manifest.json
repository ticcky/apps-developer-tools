{
  "name": "Chrome Apps Developer Tool",
  "description": "Develop and Debug Chrome apps.",
  "manifest_version": 2,
  "version": "0.1.12",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "management",
    "activityLogPrivate",
    "developerPrivate",
    "tabs",
    "debugger",
    "<all_urls>"
  ],
  "key": "",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "minimum_chrome_version": "31.0.1650.8"
}
